{% assign easylockdownNow = 'now' | date:'%s' %}
{% assign easylockdownPlacedOrder = customer.orders.first.created_at | date:'%s' %}
{% assign easylockdownOldestPlacedOrder = customer.orders.last.created_at | date:'%s' %}
{% assign easylockdownDateDifference = easylockdownNow | minus: easylockdownPlacedOrder %}
{% assign easylockdownLocationsCan = '' | split: ',' %}
{% assign easylockdownLocationsCannot = '' | split: ',' %}
{% assign easylockdownPasswordItems = '' | split: ',' %}

{% assign easylockdownSearchException = false %}
{% unless easylockdownSearchException %}
    {% if easylockdown_filter_search.object_type == 'page' %}
        {% if easylockdown_filter_search.id == 1478950932 %}
            {% assign easylockdownTmp = '584c96680' | split: ',' %}
            {% assign easylockdownPasswordItems = easylockdownPasswordItems | concat: easylockdownTmp | uniq %}
        {% endif %}
    {% endif %}
{% endunless %}

{% unless easylockdownItemSelectorOff %}<div class='easylockdown-item-selector easy-item-selector' data-eld-loc-can='{{ easylockdownLocationsCan | join: "," }}' data-eld-loc-cannot='{{ easylockdownLocationsCannot | join: "," }}' data-eld-loc-auth='{{ easylockdownPasswordItems | join: "," }}' style='display:none;'></div>{% endunless %}